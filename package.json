{
  "name": "wapp_clipboard_to_markdown",
  "version": "1.0.0",
  "description": "wapp_clipboard_to_markdown",
  "author": "Panov Anton",
  "private": true,
  "scripts": {
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt build",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt build && nuxt export && yarn pack_all_inline",
    "pack_all_inline": "DEPLOY_ENV=GH_PAGES node -r esm ./bin/packer/pack_all_inline.js",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "deploy": "DEPLOY_ENV=GH_PAGES push-dir --dir=dist"
  },
  "bin": {
    "pack_all_inline": "./bin/packer/pack_all_inline.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.1",
    "@nuxtjs/pwa": "^3.0.0-0",
    "codemirror": "^5.55.0",
    "marked": "^1.1.0",
    "nuxt": "^2.0.0",
    "turndown-plugin-gfm": "^1.0.2",
    "vue-monaco": "^1.2.1"
  },
  "devDependencies": {
    "push-dir": "^0.4.1"
  }
}
